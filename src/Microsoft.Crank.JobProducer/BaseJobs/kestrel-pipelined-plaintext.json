{
  "Default": {

    "Client": "Wrk",
    "ClientProperties": {
      "ScriptName": "pipeline",
      "PipelineDepth": 16
    },

    "PresetHeaders": "Plaintext",
    "Source": {
      "Repository": "https://github.com/aspnet/benchmarks.git",
      "BranchOrCommit": "master",
      "Project": "src/Benchmarks/Benchmarks.csproj"
    },
    "ReadyStateText": "Application started.",
    "Path": "/plaintext",

    "OutputFiles": [
      "artifacts/bin/Microsoft.AspNetCore.Server.Kestrel/Release/netcoreapp5.0/*.dll"
    ],
    "DotNetTrace": false,
    "SelfContained": true
  },

  "http2": {

    "Client": "h2load",
    "ClientProperties": {
      "Streams": 70
    },

    "PresetHeaders": "None",
    "Connections": 8,
    "Threads": 8,

    "SkipStartupLatencies": true,
    "ExtraDriverArgs": "--scheme h2c"
  },

  "grpc": {
    "Client": "h2load",
    "Path": "/grpc.testing.BenchmarkService/UnaryCall",

    "Headers": {
      "content-type": "application/grpc",
      "TE": "trailers"
    },

    "ClientProperties": {
      "RequestBody": "AAAAAAcKBVdvcmxk",
      "Streams": 70
    },

    "PresetHeaders": "None",
    "Connections": 8,
    "Threads": 8,

    "Source": {
      "Repository": "https://github.com/grpc/grpc-dotnet.git",
      "BranchOrCommit": "master",
      "Project": "perf/benchmarkapps/GrpcAspNetCoreServer/GrpcAspNetCoreServer.csproj"
    },
    
    "SkipStartupLatencies": true,
    "ExtraDriverArgs": "--scheme h2c"
  },

  "BuildInstructions": {
    "BuildCommands": [
      "./build.sh /t:Restore",
      ".dotnet/dotnet build -c Release -nodeReuse:false src/Servers/Kestrel/Kestrel/src/",
      "sh -c \"pkill dotnet || true\""
    ]
  }
}
